{
  "pathFilter": {
    "deniedPaths" : [
      { "path": "{prefix:(//[\\w-]+)?}/api/engine/.*", "methods" : "*" },
      { "path": "{prefix:(//[\\w-]+)?}/api/cockpit/.*", "methods" : "*" },
      { "path": "{prefix:(//[\\w-]+)?}/api/tasklist/.*", "methods" : "*" },
      { "path": "{prefix:(//[\\w-]+)?}/api/admin/.*", "methods" : "*" },
      { "path": "{prefix:(//[\\w-]+)?}/app/tasklist/{engine}/.*", "methods" : "*" },
      { "path": "{prefix:(//[\\w-]+)?}/app/cockpit/{engine}/.*", "methods" : "*" },
      { "path": "{prefix:(//[\\w-]+)?}/app/welcome/{engine}/.*", "methods" : "*" },
      { "path": "{prefix:(//[\\w-]+)?}/app/admin/{engine}/.*", "methods" : "*" }
    ],
    "allowedPaths" : [
      { "path": "{prefix:(//[\\w-]+)?}/api/engine/engine/", "methods" : "GET" },
      { "path": "{prefix:(//[\\w-]+)?}/api/{app:cockpit}/plugin/{plugin}/static/.*", "methods" : "GET" },
      { "path": "{prefix:(//[\\w-]+)?}/api/{app:cockpit}/plugin/{plugin}/{engine}/.*", "methods" : "*", "authorizer" : "org.camunda.bpm.webapp.impl.security.filter.EngineRequestAuthorizer" },
      { "path": "{prefix:(//[\\w-]+)?}/api/engine/engine/{engine}/identity/password-policy", "methods" : "GET" },
      { "path": "{prefix:(//[\\w-]+)?}/api/engine/engine/{engine}/identity/password-policy", "methods" : "POST" },
      { "path": "{prefix:(//[\\w-]+)?}/api/admin/auth/user/{engine}", "methods" : "GET" },
      { "path": "{prefix:(//[\\w-]+)?}/api/admin/auth/user/{engine}/logout", "methods" : "POST" },
      { "path": "{prefix:(//[\\w-]+)?}/api/admin/auth/user/{engine}/login/{app}", "methods" : "POST" },
      { "path": "{prefix:(//[\\w-]+)?}/api/admin/auth/user/{engine}/.*", "methods" : "*", "authorizer" : "org.camunda.bpm.webapp.impl.security.filter.EngineRequestAuthorizer" },
      { "path": "{prefix:(//[\\w-]+)?}/api/admin/setup/{engine}/user/create", "methods" : "POST" },
      { "path": "{prefix:(//[\\w-]+)?}/api/admin/setup/{engine}/.*", "methods" : "*", "authorizer" : "org.camunda.bpm.webapp.impl.security.filter.EngineRequestAuthorizer" },
      { "path": "{prefix:(//[\\w-]+)?}/api/{app:admin}/plugin/{plugin}/static/.*", "methods" : "GET" },
      { "path": "{prefix:(//[\\w-]+)?}/api/{app:admin}/plugin/{plugin}/{engine}/.*", "methods" : "*", "authorizer" : "org.camunda.bpm.webapp.impl.security.filter.EngineRequestAuthorizer" },
      { "path": "{prefix:(//[\\w-]+)?}/api/{app:tasklist}/plugin/{plugin}/static/.*", "methods" : "GET" },
      { "path": "{prefix:(//[\\w-]+)?}/api/{app:tasklist}/plugin/{plugin}/{engine}/.*", "methods" : "*", "authorizer" : "org.camunda.bpm.webapp.impl.security.filter.EngineRequestAuthorizer" },
      { "path": "{prefix:(//[\\w-]+)?}/api/engine/engine/{engine}/.*", "methods" : "*", "authorizer" : "org.camunda.bpm.webapp.impl.security.filter.EngineRequestAuthorizer" },
      { "path": "{prefix:(//[\\w-]+)?}/app/{app:cockpit}/{engine}/.*", "methods" : "*", "authorizer" : "org.camunda.bpm.webapp.impl.security.filter.ApplicationRequestAuthorizer" },
      { "path": "{prefix:(//[\\w-]+)?}/app/{app:tasklist}/{engine}/.*", "methods" : "*", "authorizer" : "org.camunda.bpm.webapp.impl.security.filter.ApplicationRequestAuthorizer" },
      { "path": "{prefix:(//[\\w-]+)?}/app/{app:welcome}/{engine}/.*", "methods" : "*", "authorizer" : "org.camunda.bpm.webapp.impl.security.filter.ApplicationRequestAuthorizer" },
      { "path": "{prefix:(//[\\w-]+)?}/app/{app:admin}/{engine}/.*", "methods" : "*", "authorizer" : "org.camunda.bpm.webapp.impl.security.filter.ApplicationRequestAuthorizer" }
    ]
  }
}
